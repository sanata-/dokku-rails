<%= render 'dropdown' %>
<nav>
  <div class="nav-wrapper">
    <a><%= link_to 'Home', root_path, class: 'brand-logo' %></a>
    <ul class="right hide-on-med-and-down">
      <li><%= link_to 'About', pages_about_path %></li>
      <li><%= link_to 'Help', pages_help_path %></li>
      <!-- Dropdown Trigger -->

      <% if user_signed_in? %>
      <li><a class="dropdown-button" href="#" data-activates="dropdown1"><%= current_user.email %><i class="material-icons right">arrow_drop_down</i></a></li>
      <% else %>
      <li><%= link_to "Sign up", new_user_registration_path, :class => 'navbar-link'  %></li>
      <li><%= link_to "Login", new_user_session_path, :class => 'navbar-link'  %></li>
      <% end %>
    </ul>
  </div>
</nav>
